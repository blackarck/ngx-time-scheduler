{
    "name": "ngx-time-scheduler-resize",
    "version": "1.6.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "postinstall": "ngcc",
        "ng:build:demo": "ng build --aot --prod --base-href=https://blackarck.github.io/ngx-time-scheduler/",
        "deploy": "npm run ng:build:demo && gh-pages -d dist/demo",
        "build_lib": "ng build NgxTimeSchedulerResize --prod",
        "copy_license": "cp ./LICENSE ./dist/ngx-time-scheduler-resize",
        "copy_readme": "cp ./README.md ./dist/ngx-time-scheduler-resize",
        "copy_files": "npm run copy_license && npm run copy_readme",
        "npm_pack": "cd dist/ngx-time-scheduler-resize && npm pack",
        "package": "npm run build_lib && npm run copy_files && npm run npm_pack",
        "publish": "cd dist/ngx-time-scheduler-resize && npm publish"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^11.0.2",
        "@angular/cdk": "^11.0.1",
